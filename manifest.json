{
  "manifest_version": 3,
  "name": "MyEpitech",
  "version": "2.4",
  "description": "Restore the old percentage of My Epitech !",
  "host_permissions": [
    "*://my.epitech.eu/*",
    "*://api.epitest.eu/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "*://my.epitech.eu/*"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  }
}
